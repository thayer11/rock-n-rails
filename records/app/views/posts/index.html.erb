<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>String</th>
      <th>Year</th>
      <th>Integer</th>
      <th>Conver art</th>
      <th>String</th>
      <th>Song count</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.artist %></td>
        <td><%= post.string %></td>
        <td><%= post.year %></td>
        <td><%= post.integer %></td>
        <td><%= post.conver_art %></td>
        <td><%= post.string %></td>
        <td><%= post.song_count %></td>
        <td><%= post.integer %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
